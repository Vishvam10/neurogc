{
  "profile_interval": 1.0,
  "server_ports": {
    "with_neurogc": 8001,
    "without_neurogc": 8002,
    "metrics_server": 8003
  },
  "default_model": "lstm",
  "gc_threshold": 0.7,
  "thresholds": {
    "cpu": 80.0,
    "memory": 75.0,
    "disk_read": 100000000,
    "disk_write": 100000000,
    "net_sent": 10000000,
    "net_recv": 10000000,
    "p95": 500.0,
    "p99": 1000.0
  },
  "csv_path": "profiler_data.csv",
  "model_path": "gc_model.pth",
  "locust": {
    "host_with_gc": "http://localhost:8001",
    "host_without_gc": "http://localhost:8002",
    "metrics_server_url": "http://localhost:8003"
  },
  "models": {
    "lstm": {
      "input_size": 10,
      "hidden_size": 64,
      "num_layers": 10,
      "sequence_length": 100,
      "epochs": 100,
      "learning_rate": 0.001,
      "batch_size": 32
    },
    "transformer": {
      "d_model": 64,
      "nhead": 4,
      "num_layers": 2,
      "sequence_length": 10,
      "epochs": 100,
      "learning_rate": 0.001,
      "batch_size": 32
    },
    "feedforward": {
      "hidden_sizes": [64, 32],
      "lookback": 5,
      "epochs": 100,
      "learning_rate": 0.001,
      "batch_size": 32
    },
    "classical": {
      "algorithm": "random_forest",
      "n_estimators": 100,
      "max_depth": null,
      "lookback": 5
    }
  }
}